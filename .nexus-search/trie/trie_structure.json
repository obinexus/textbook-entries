{
  "trie_type": "avl_based",
  "total_tokens": 3231,
  "algorithms_supported": ["bfs", "a_star", "topological", "regex_match"],
  "search_patterns": {
    "exact_match": "\\b{token}\\b",
    "fuzzy_match": ".*{token}.*",
    "prefix_match": "{token}.*",
    "wildcard_match": "*.{ext}"
